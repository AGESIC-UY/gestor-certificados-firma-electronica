<?xml version="1.0" encoding="UTF-8"?>
<faces-config version="2.0" xmlns="http://java.sun.com/xml/ns/javaee"
			  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
			  xsi:schemaLocation="
        http://java.sun.com/xml/ns/javaee
        http://java.sun.com/xml/ns/javaee/web-facesconfig_2_0.xsd">

	<application>
		<locale-config>
			<default-locale>es</default-locale>
		</locale-config>
		<resource-bundle>
			<base-name>com.sofis.generico.web.mensajes.Labels</base-name>
			<var>labels</var>
		</resource-bundle>
	</application>
	
	<navigation-rule>
		<from-view-id>*</from-view-id>
		<navigation-case>
			<from-outcome>NUEVO</from-outcome>
			<to-view-id>/paginasPrivadas/nuevoCertificadoCliente.jsf</to-view-id>
			<redirect/>
		</navigation-case>
	</navigation-rule>
        
        <navigation-rule>
		<from-view-id>*</from-view-id>
		<navigation-case>
			<from-outcome>LISTAR</from-outcome>
			<to-view-id>/paginasPrivadas/listarCertificadoCliente.jsf</to-view-id>
			<redirect/>
		</navigation-case>
	</navigation-rule>
        
        <navigation-rule>
		<from-view-id>*</from-view-id>
		<navigation-case>
			<from-outcome>VALIDAR</from-outcome>
			<to-view-id>/paginasPublicas/validarCertificadoCliente.jsf</to-view-id>
			<redirect/>
		</navigation-case>
	</navigation-rule>
        
        
	
</faces-config>
